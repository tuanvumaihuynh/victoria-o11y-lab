{
  "uid": "http-api-overview",
  "title": "HTTP API Overview",
  "tags": ["http", "api"],
  "timezone": "browser",
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "method",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "victoriametrics"
        },
        "query": "label_values(http_requests_total, method)",
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "refresh": 2
      },
      {
        "name": "endpoint",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "victoriametrics"
        },
        "query": "label_values(http_requests_total{method=~\"$method\"}, endpoint)",
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "refresh": 2
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Stats",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Total Requests",
      "datasource": {
        "type": "prometheus",
        "uid": "victoriametrics"
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(http_requests_total{method=~\"$method\", endpoint=~\"$endpoint\"})",
          "legendFormat": "Total"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Request Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "victoriametrics"
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval]))",
          "legendFormat": "req/s"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Avg Latency",
      "datasource": {
        "type": "prometheus",
        "uid": "victoriametrics"
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_sum{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval])) / sum(rate(http_request_duration_seconds_count{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval]))",
          "legendFormat": "avg"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 1 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Inflight Requests",
      "datasource": {
        "type": "prometheus",
        "uid": "victoriametrics"
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "expr": "http_inflight_requests",
          "legendFormat": "inflight"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Request Rate",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Request Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "victoriametrics"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval]))",
          "legendFormat": "req/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Request Rate by Endpoint",
      "datasource": {
        "type": "prometheus",
        "uid": "victoriametrics"
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "sum by (method, endpoint)(rate(http_requests_total{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval]))",
          "legendFormat": "{{method}} {{endpoint}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Latency",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Latency Percentiles",
      "datasource": {
        "type": "prometheus",
        "uid": "victoriametrics"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Latency by Endpoint (p95)",
      "datasource": {
        "type": "prometheus",
        "uid": "victoriametrics"
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, method, endpoint)(rate(http_request_duration_seconds_bucket{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval])))",
          "legendFormat": "{{method}} {{endpoint}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Inflight",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Inflight Requests",
      "datasource": {
        "type": "prometheus",
        "uid": "victoriametrics"
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "http_inflight_requests",
          "legendFormat": "inflight"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      }
    }
  ],
  "schemaVersion": 39,
  "version": 1
}
