        { "expr": "go_memstats_alloc_bytes_total", "refId": "A" }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": { "steps": [{ "color": "blue", "value": null }] }
        }
      }
    },
    {
      "type": "stat",
      "id": 31,
      "title": "GC Cycles Total",
