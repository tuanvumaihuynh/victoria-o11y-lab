{
  "uid": "go-app-overview",
  "title": "Go Application Overview",
  "tags": ["go", "application"],
  "timezone": "browser",
  "refresh": "10s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "schemaVersion": 38,
  "version": 1,
  "templating": {
    "list": [
      {
        "name": "method",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "victoriametrics" },
        "query": "label_values(http_requests_total, method)",
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "current": { "selected": true, "text": "All", "value": "$__all" }
      },
      {
        "name": "endpoint",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "victoriametrics" },
        "query": "label_values(http_requests_total{method=~\"$method\"}, endpoint)",
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "current": { "selected": true, "text": "All", "value": "$__all" }
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "id": 1,
      "title": "HTTP Request",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 2,
      "title": "HTTP Request Rate Per Minute",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum by (method, endpoint)(rate(http_requests_total{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval])) * 60",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqpm",
          "custom": { "lineWidth": 1, "fillOpacity": 0 }
        }
      },
      "options": {
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "stat",
      "id": 3,
      "title": "Total Requests",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum(increase(http_requests_total{method=~\"$method\", endpoint=~\"$endpoint\"}[$__range]))",
          "instant": true,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": { "steps": [{ "color": "green", "value": null }] }
        }
      }
    },
    {
      "type": "stat",
      "id": 4,
      "title": "Request Rate",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": { "steps": [{ "color": "blue", "value": null }] }
        }
      }
    },
    {
      "type": "stat",
      "id": 5,
      "title": "Avg Latency",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 5 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_sum{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval])) / sum(rate(http_request_duration_seconds_count{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 1 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "id": 6,
      "title": "Inflight Requests",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 5 },
      "targets": [
        {
          "expr": "http_inflight_requests",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "id": 7,
      "title": "HTTP Requests Per Second",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "targets": [
        {
          "expr": "sum by (method, endpoint)(rate(http_requests_total{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval]))",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "lineWidth": 1, "fillOpacity": 0 }
        }
      },
      "options": {
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "timeseries",
      "id": 8,
      "title": "Latency Percentiles",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le)(rate(http_request_duration_seconds_bucket{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le)(rate(http_request_duration_seconds_bucket{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval])))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le)(rate(http_request_duration_seconds_bucket{method=~\"$method\", endpoint=~\"$endpoint\"}[$__rate_interval])))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "lineWidth": 1, "fillOpacity": 0 }
        }
      },
      "options": {
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "table",
      "id": 9,
      "title": "HTTP Requests Count by Endpoint",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 17 },
      "targets": [
        {
          "expr": "sum by (method, endpoint)(increase(http_requests_total{method=~\"$method\", endpoint=~\"$endpoint\"}[$__range]))",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "expr": "sum by (method, endpoint)(increase(http_request_duration_seconds_sum{method=~\"$method\", endpoint=~\"$endpoint\"}[$__range])) / sum by (method, endpoint)(increase(http_request_duration_seconds_count{method=~\"$method\", endpoint=~\"$endpoint\"}[$__range]))",
          "format": "table",
          "instant": true,
          "refId": "B"
        }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": {
              "method": "Method",
              "endpoint": "Endpoint",
              "Value #A": "Total Requests",
              "Value #B": "Avg Duration"
            }
          }
        }
      ],
      "options": {
        "cellHeight": "sm",
        "footer": { "show": false },
        "showHeader": true
      },
      "fieldConfig": {
        "defaults": { "custom": { "align": "center" } },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Avg Duration" },
            "properties": [{ "id": "unit", "value": "s" }]
          }
        ]
      }
    },
    {
      "type": "bargauge",
      "id": 10,
      "title": "Average HTTP Request Duration by Endpoint",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 17 },
      "targets": [
        {
          "expr": "sum by (method, endpoint)(increase(http_request_duration_seconds_sum{method=~\"$method\", endpoint=~\"$endpoint\"}[$__range])) / sum by (method, endpoint)(increase(http_request_duration_seconds_count{method=~\"$method\", endpoint=~\"$endpoint\"}[$__range]))",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ],
      "options": {
        "displayMode": "lcd",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showUnfilled": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "continuous-GrYlRd" }
        }
      }
    },
    {
      "type": "row",
      "id": 11,
      "title": "Quick Facts",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 25 },
      "collapsed": false
    },
    {
      "type": "stat",
      "id": 12,
      "title": "Uptime",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 26 },
      "targets": [
        {
          "expr": "(time() - process_start_time_seconds) / 3600",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 2,
          "thresholds": { "steps": [{ "color": "green", "value": null }] }
        }
      }
    },
    {
      "type": "stat",
      "id": 13,
      "title": "Open File Descriptors",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 26 },
      "targets": [
        { "expr": "process_open_fds", "refId": "A" }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 900 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "id": 14,
      "title": "Goroutines",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 26 },
      "targets": [
        { "expr": "go_goroutines", "refId": "A" }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 2000 }
            ]
          }
        }
      }
    },
    {
      "type": "row",
      "id": 15,
      "title": "Go Runtime",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 34 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 16,
      "title": "Goroutines",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 35 },
      "targets": [
        { "expr": "go_goroutines", "legendFormat": "goroutines", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 1, "fillOpacity": 10 },
          "color": { "mode": "fixed", "fixedColor": "green" }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "timeseries",
      "id": 17,
      "title": "OS Threads",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 35 },
      "targets": [
        { "expr": "go_threads", "legendFormat": "threads", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 1, "fillOpacity": 10 },
          "color": { "mode": "fixed", "fixedColor": "blue" }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "timeseries",
      "id": 18,
      "title": "GC Pause Duration",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 43 },
      "targets": [
        {
          "expr": "go_gc_duration_seconds{quantile=\"0.5\"}",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "go_gc_duration_seconds{quantile=\"0.75\"}",
          "legendFormat": "p75",
          "refId": "B"
        },
        {
          "expr": "go_gc_duration_seconds{quantile=\"1\"}",
          "legendFormat": "p100 (max)",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "lineWidth": 1, "fillOpacity": 0 }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "timeseries",
      "id": 19,
      "title": "GC CPU Fraction",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 43 },
      "targets": [
        {
          "expr": "go_memstats_gc_cpu_fraction",
          "legendFormat": "GC CPU fraction",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "custom": { "lineWidth": 1, "fillOpacity": 10 }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "timeseries",
      "id": 20,
      "title": "Heap Memory",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 51 },
      "targets": [
        { "expr": "go_memstats_heap_alloc_bytes", "legendFormat": "Alloc", "refId": "A" },
        { "expr": "go_memstats_heap_inuse_bytes", "legendFormat": "In Use", "refId": "B" },
        { "expr": "go_memstats_heap_idle_bytes", "legendFormat": "Idle", "refId": "C" },
        { "expr": "go_memstats_heap_sys_bytes", "legendFormat": "Sys", "refId": "D" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "lineWidth": 1, "fillOpacity": 0 }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "timeseries",
      "id": 21,
      "title": "Stack Memory",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 51 },
      "targets": [
        { "expr": "go_memstats_stack_inuse_bytes", "legendFormat": "Stack In Use", "refId": "A" },
        { "expr": "go_memstats_stack_sys_bytes", "legendFormat": "Stack Sys", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "lineWidth": 1, "fillOpacity": 0 }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "row",
      "id": 22,
      "title": "CPU Observability",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 59 },
      "collapsed": false
    },
    {
