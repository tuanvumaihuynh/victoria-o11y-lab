        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "timeseries",
      "id": 29,
      "title": "Next GC Target",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 77 },
      "targets": [
        { "expr": "go_memstats_next_gc_bytes", "legendFormat": "Next GC target", "refId": "A" },
        { "expr": "go_memstats_heap_alloc_bytes", "legendFormat": "Heap Alloc", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "min": 0,
