        { "expr": "rate(process_cpu_seconds_total[$__rate_interval])", "refId": "A" }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 0.8 }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "id": 24,
      "title": "CPU Usage Rate",
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 60 },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[$__rate_interval])",
          "legendFormat": "CPU Usage",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "custom": { "lineWidth": 1, "fillOpacity": 10 }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "row",
      "id": 25,
      "title": "Memory Observability",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 68 },
