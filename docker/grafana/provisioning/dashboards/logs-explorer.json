{
  "uid": "logs-explorer",
  "title": "Logs Explorer",
  "tags": ["logs"],
  "timezone": "browser",
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "container",
        "type": "custom",
        "query": "app-api-1,app-migrate-1",
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "level",
        "type": "custom",
        "query": "trace,debug,info,warn,error,fatal",
        "includeAll": true,
        "allValue": "*",
        "multi": false,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Log Volume",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Log Volume",
      "datasource": {
        "type": "victoriametrics-logs-datasource",
        "uid": "victorialogs"
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "{container_name=~\"$container\"} level:$level | stats by (_time:$__interval) count() as count",
          "legendFormat": "count"
        }
      ],
      "transformations": [
        {
          "id": "convertFieldType",
          "options": {
            "fields": [
              {
                "name": "_time",
                "destinationType": "time"
              },
              {
                "name": "count",
                "destinationType": "number"
              }
            ]
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50,
            "lineWidth": 1,
            "stacking": {
              "mode": "normal"
            }
          },
          "color": {
            "mode": "fixed",
            "fixedColor": "blue"
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Logs",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "collapsed": false
    },
    {
      "type": "logs",
      "title": "Logs",
      "datasource": {
        "type": "victoriametrics-logs-datasource",
        "uid": "victorialogs"
      },
      "gridPos": { "h": 16, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "{container_name=~\"$container\"} level:$level"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    }
  ],
  "schemaVersion": 39,
  "version": 1
}
